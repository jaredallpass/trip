<p id="notice"><%= notice %></p>

<h1>Music Sub Categories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @music_sub_categories.each do |music_sub_category| %>
      <tr>
        <td><%= link_to 'Show', music_sub_category %></td>
        <td><%= link_to 'Edit', edit_music_sub_category_path(music_sub_category) %></td>
        <td><%= link_to 'Destroy', music_sub_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Music Sub Category', new_music_sub_category_path %>
