<p id="notice"><%= notice %></p>

<h1>Image Sub Categories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_sub_categories.each do |image_sub_category| %>
      <tr>
        <td><%= link_to 'Show', image_sub_category %></td>
        <td><%= link_to 'Edit', edit_image_sub_category_path(image_sub_category) %></td>
        <td><%= link_to 'Destroy', image_sub_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image Sub Category', new_image_sub_category_path %>
