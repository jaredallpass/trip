<p id="notice"><%= notice %></p>

<h1>Word Porns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @word_porns.each do |word_porn| %>
      <tr>
        <td><%= link_to 'Show', word_porn %></td>
        <td><%= link_to 'Edit', edit_word_porn_path(word_porn) %></td>
        <td><%= link_to 'Destroy', word_porn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Word Porn', new_word_porn_path %>
